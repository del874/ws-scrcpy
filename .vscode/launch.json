{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug ws-scrcpy",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/src/server/index.ts",
            "preLaunchTask": "npm: dist:dev",
            "outFiles": ["${workspaceFolder}/dist/**/*.js"],
            "sourceMaps": true,
            "cwd": "${workspaceFolder}"
        }
    ]
}
